<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Express Practice Lab</title>
  </head>
  <body>
    <h1>All Students</h1>
    <ul>
      <% students.forEach(function(s) { %>
      <li>
        <%= s.name %> 
          -  
        <%= s.graduate ? 'graduated' : 'not a graduate' %>
      </li>
      <% }); %>
    </ul>
  </body>
</html>
